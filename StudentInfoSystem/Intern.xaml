<Window x:Class="StudentInfoSystem.Intern"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentInfoSystem"
        mc:Ignorable="d"
        Title="Intern" Height="300" Width="300">
    <Grid>
        <TextBlock x:Name="InternEntry" Margin="10,42,151.6,0" TextWrapping="Wrap" Text="{Binding StudIntern}" VerticalAlignment="Top" Height="24" Width="132"/>
        <Label Content="{Binding CurrentStudent.Name}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="32" Width="71"/>
        <TextBox HorizontalAlignment="Left" x:Name="txtInt" Height="23" Margin="10,66,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="add" Content="Add" HorizontalAlignment="Left" Margin="135,66,0,0" VerticalAlignment="Top" Width="75" Click="add_Click_1"/>

    </Grid>
</Window>
